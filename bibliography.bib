@book{butlerGenderTroubleFeminism1990,
	title = {Gender Trouble: Feminism and the Subversion of Identity},
	isbn = {978-0-415-90042-3},
	shorttitle = {Gender Trouble},
	abstract = {Since its publication in 1990, "Gender Trouble" has become one of the key works of contemporary feminist theory, and an essential work for anyone interested in the study of gender, queer theory, or the politics of sexuality in culture. As Judith Butler writes in the major essay that stands as preface to the new edition, one point of "Gender Trouble" was 'not to prescribe a new gendered way of life, but to open of the field of possibility for gender.' Widely taught, and widely debated, "Gender Trouble" continues to offer a powerful critique of heteronormativity and of the function of gender in the modern world. Judith Butler's new preface situates "Gender Trouble" within the past decade of work on gender, and counters some common misconceptions about the book and its aims.},
	pagetotal = {196},
	publisher = {Routledge},
	author = {Butler, Judith},
	date = {1990},
	langid = {english},
	note = {Annotation: Fundamental theory for gender power structure: Judith Butler's theory, compared to Goffman's one, challenged the power structure more, that Gender, here in their ideas, is performed under the heteronormative social structure.
Relationship between structure and gender representation: Despite the main idea that gender is constructed, an inner idea of Butler is that the gender performance itself is constructed by social structures. If we take gender representation in paintings as a kind of gender performance, it seems important to see the relationship between gender power structure and representation in paintings.
Application to my research: How gender power structure would influence gender representations in paintings is not clear. I aim to operationalize gender representation via machine learning as well as the gender power structure by social network metrics, to test this correlation.},
	keywords = {Social Science / Gender Studies, Psychology / General, Psychology / Human Sexuality, Social Science / Feminism \& Feminist Theory},
}

