
@article{guilbeaultOnlineImagesAmplify2024,
	title = {Online images amplify gender bias},
	volume = {626},
	rights = {2024 The Author(s)},
	issn = {1476-4687},
	url = {https://www.nature.com/articles/s41586-024-07068-x},
	doi = {10.1038/s41586-024-07068-x},
	abstract = {Each year, people spend less time reading and more time viewing images1, which are proliferating online2–4. Images from platforms such as Google and Wikipedia are downloaded by millions every day2,5,6, and millions more are interacting through social media, such as Instagram and {TikTok}, that primarily consist of exchanging visual content. In parallel, news agencies and digital advertisers are increasingly capturing attention online through the use of images7,8, which people process more quickly, implicitly and memorably than text9–12. Here we show that the rise of images online significantly exacerbates gender bias, both in its statistical prevalence and its psychological impact. We examine the gender associations of 3,495 social categories (such as ‘nurse’ or ‘banker’) in more than one million images from Google, Wikipedia and Internet Movie Database ({IMDb}), and in billions of words from these platforms. We find that gender bias is consistently more prevalent in images than text for both female- and male-typed categories. We also show that the documented underrepresentation of women online13–18 is substantially worse in images than in text, public opinion and {US} census data. Finally, we conducted a nationally representative, preregistered experiment that shows that googling for images rather than textual descriptions of occupations amplifies gender bias in participants’ beliefs. Addressing the societal effect of this large-scale shift towards visual communication will be essential for developing a fair and inclusive future for the internet.},
	pages = {1049--1055},
	number = {8001},
	journaltitle = {Nature},
	author = {Guilbeault, Douglas and Delecourt, Solène and Hull, Tasker and Desikan, Bhargav Srinivasa and Chu, Mark and Nadler, Ethan},
	urldate = {2025-03-05},
	date = {2024-02},
	langid = {english},
	note = {Annotation: Visual data and gender bias: This study evaluates over one million online images across major platforms like Google, Wikipedia, and {IMDb}, analyzing gender associations in 3,495 social categories. It finds that images show stronger gender bias than both textual descriptions and official statistics (e.g., {US} Census). Using experiments, the research found that images would have higher chance to introduce gender stereotype than texts.
Relevance to my study: While the study investigates contemporary media, its relevance lies in the methodological and conceptual assertion that visual forms (more than text) encode and transmit gender biases. This supports the central claim of my research—that visual cultural products like paintings would be influenced by societal gender structures and perpetuate them, even more than the texts-ish cultural products.},
	keywords = {Sociology, Communication, Human behaviour, not painting},
	file = {Guilbeault et al_2024_Online images amplify gender bias.pdf:/Users/pootaatoos/Zotero/storage/BRKNMXVM/Guilbeault et al_2024_Online images amplify gender bias.pdf:application/pdf},
}

@article{adukiaWhatWeTeach2023,
	title = {What We Teach About Race and Gender: Representation in Images and Text of Children’s Books},
	volume = {138},
	issn = {0033-5533},
	url = {https://doi.org/10.1093/qje/qjad028},
	doi = {10.1093/qje/qjad028},
	shorttitle = {What We Teach About Race and Gender},
	abstract = {Books shape how children learn about society and norms, in part through representation of different characters. We use computational tools to characterize representation in children’s books widely read in homes, classrooms, and libraries over the past century and describe economic forces that may contribute to these patterns. We introduce new artificial intelligence methods for systematically converting images into data. We apply these tools, alongside text analysis methods, to measure skin color, race, gender, and age in the content of these books, documenting what has changed and what has endured over time. We find underrepresentation of Black and Latinx people in the most influential books, relative to their population shares, though representation of Black individuals increases over time. Females are also increasingly present but appear less often in text than in images, suggesting greater symbolic inclusion in pictures than substantive inclusion in stories. Characters in these influential books have lighter average skin color than in other books, even after conditioning on race, and children are depicted with lighter skin color than adults on average. We present empirical analysis of related economic behavior to better understand the representation we find in these books. On the demand side, we show that people consume books that center their own identities and that the types of children’s books purchased correlate with local political beliefs. On the supply side, we document higher prices for books that center nondominant social identities and fewer copies of these books in libraries that serve predominantly White communities.},
	pages = {2225--2285},
	number = {4},
	journaltitle = {The Quarterly Journal of Economics},
	shortjournal = {The Quarterly Journal of Economics},
	author = {Adukia, Anjali and Eble, Alex and Harrison, Emileigh and Runesha, Hakizumwami Birali and Szasz, Teodora},
	urldate = {2025-03-05},
	date = {2023-11-01},
	note = {Annotation: Analysis of visual and textual representation in Children's literature: This paper uses {AI}-based computational tools to examine race and gender representations in over 100 years of children's literature. The authors analyze both textual and visual data, identifying underrepresentation and stereotypical portrayals of women and racial minorities across time. While female characters have become more visually prominent over time, their roles in text remain limited—suggesting symbolic inclusion without substantive narrative presence.
Methodological significance: The study introduces tools for detecting race, gender, age, and skin tone in illustrated images. These methods offer a possible model for my visual analysis of paintings.
Relevance to my research: Although focused on children’s books, the study highlights the persistence of gender bias in cultural productions—a pattern I investigate within fine art.},
	keywords = {not painting},
	file = {Adukia et al_2023_What We Teach About Race and Gender.pdf:/Users/pootaatoos/Zotero/storage/IX62KNHM/Adukia et al_2023_What We Teach About Race and Gender.pdf:application/pdf},
}

@inproceedings{surapaneniExploringThemesBias2020,
	location = {Charlottesville, {VA}, {USA}},
	title = {Exploring Themes and Bias in Art using Machine Learning Image Analysis},
	rights = {https://ieeexplore.ieee.org/Xplorehelp/downloads/license-information/{IEEE}.html},
	isbn = {978-1-72817-145-6},
	url = {https://ieeexplore.ieee.org/document/9106656/},
	doi = {10.1109/SIEDS49339.2020.9106656},
	abstract = {Machine learning and computer vision have been applied for image recognition of art objects such as paintings, sculpture images etc. In particular, deep learning methods for image classiﬁcation in art have been used to improve user engagement by providing access to accurately labelled and classiﬁed art objects. As an increasing number of notable museums turn towards creating open access collections, alternatives to the use of laborious human annotating methods are needed. This paper focuses on the Open Access initiative of The Metropolitan Museum of Art (The Met) which was launched in 2017 in an effort to expand The Met’s reach and presence. The museum now provides a select dataset of information on more than 470,000 artworks in its collection for unrestricted commercial and noncommercial use. However, with a widely accessible collection, the Met now faces the problem of how to enhance the user experience via access to accurately labelled art. This paper focuses on machine learning methods with applicability to automated classiﬁcation of images obtained from The Met’s online collection. We aimed to: 1) Compare three different convolutional neural networks {ResNet} 50, {ResNet} 101, and Inception-{ResNet}-V2- using human annotated data, 2) Add transparency and interpretability to our models by using Gradient Weighted Class Activation Maps ({GRAD}-cams) and to explore bias in gender labels and 3) implement a multi-label classiﬁcation model using {ResNet} 50. Future work would include the use of unsupervised clustering methods / auto-encoders to explore additional themes in the data. Other extensions of this work would include exploring methods to implement ﬁne grained visual categorization, to mitigate bias, and to address the limitations associated with culture and stylistic interpretations. Deep learning techniques for art image classiﬁcation may also help detect consistent features of bias in human annotated art.},
	eventtitle = {2020 Systems and Information Engineering Design Symposium ({SIEDS})},
	pages = {1--6},
	booktitle = {2020 Systems and Information Engineering Design Symposium ({SIEDS})},
	publisher = {{IEEE}},
	author = {Surapaneni, Sudeepti and Syed, Sana and Lee, Logan Yoonhyuk},
	urldate = {2025-03-05},
	date = {2020-04},
	langid = {english},
	note = {Annotation: Machine learning for visual bias detection in art: This paper explores the use of convolutional neural networks ({CNNs}) to analyze large-scale art collections. The authors evaluate {ResNet}50, {ResNet}101, and Inception-{ResNet}-V2 architectures to classify themes in artworks and investigate potential gender bias in labels and representations.
Relevance to my study: This work directly informs the possible techniques of my project. Their approach to labeling gender via machine learning, and concerns about embedded biases, are especially crucial. The methods can be adapted to identify character roles, postures, and prominence in paintings, helping quantify dimensions like “Size” and “Role” from Goffman’s typology.},
	keywords = {painting},
	file = {Surapaneni et al_2020_Exploring Themes and Bias in Art using Machine Learning Image Analysis.pdf:/Users/pootaatoos/Zotero/storage/7KDRZZBG/Surapaneni et al_2020_Exploring Themes and Bias in Art using Machine Learning Image Analysis.pdf:application/pdf},
}

@article{karsayIncreasinglySexySexuality2019,
	title = {Increasingly sexy? Sexuality and sexual objectification in popular music videos, 1995–2016},
	volume = {8},
	issn = {2160-4142},
	doi = {10.1037/ppm0000221},
	shorttitle = {Increasingly sexy?},
	abstract = {We conducted a longitudinal visual content analysis of 462 popular music videos from 1995 to 2016 analyzing the depictions of sexuality, sexual objectification, and ambiguous sexual expression. Our analysis paid attention to the gender and race of the musical artist in each video, as well as the video’s music genre. Regression analyses revealed that depicted sexuality and sexual objectification did not change during the period studied. However, ambiguous sexual expression, including sexual gestures, sexual poses, and sexual facial expressions, did increase over time. Furthermore, female music artists were more often portrayed as sexually objectified than male artists. Male artists were more likely shown to objectify other individuals compared with female artists. Moreover, Black and non-White artists were more often shown presenting ambiguous sexual expression than White/Caucasian artists. No significant differences occurred when comparing music videos from the R\&B/hip-hop/rap genres with music videos from the pop genre. Our findings are discussed in the light of objectification theory and social–cognitive theory leading to suggestions for future research. ({PsycINFO} Database Record (c) 2020 {APA}, all rights reserved)},
	pages = {346--357},
	number = {4},
	journaltitle = {Psychology of Popular Media Culture},
	author = {Karsay, Kathrin and Matthes, Jörg and Buchsteiner, Lisa and Grosser, Veronika},
	date = {2019},
	note = {Annotation: Longitudinal study on visual sexualization: This paper presents a visual content analysis of 462 music videos over 21 years (1995–2016), focusing on representations of sexuality, sexual objectification, and gendered power dynamics. It quantifies these themes through coding of gestures, poses, expressions, and gaze. Although explicit sexual content did not increase over time, ambiguous sexual expressions (e.g., suggestive gestures or poses) did, and women were significantly more objectified than men.
Empirical study expanding Goffman's idea: The findings extend objectification theory and show how visual media reinforce traditional gender hierarchies through posture and presentation—resonating with Goffman's "Gender Advertisements" typologies like Ritualization of Subordination and Feminine Touch.
Relevance to my research: The study is crucial in affirming that postural representation (body cant, gaze, gesture) can encode gender hierarchy. These dimensions are directly applicable to painting analysis, where character positioning and activity serve as visual proxies for power.},
	keywords = {Music, Sexuality, Content Analysis, Digital Video, Objectification, not painting},
	file = {Karsay et al_2019_Increasingly sexy.pdf:/Users/pootaatoos/Zotero/storage/FWELH3PN/Karsay et al_2019_Increasingly sexy.pdf:application/pdf;Snapshot:/Users/pootaatoos/Zotero/storage/I9UGDLCG/2018-64671-001.html:text/html},
}

@inproceedings{szaszMeasuringRepresentationRace2022,
	title = {Measuring Representation of Race, Gender, and Age in Children’s Books: Face Detection and Feature Classification in Illustrated Images},
	url = {https://ieeexplore.ieee.org/document/9706964},
	doi = {10.1109/WACV51458.2022.00343},
	shorttitle = {Measuring Representation of Race, Gender, and Age in Children’s Books},
	abstract = {Images in children’s books convey messages about society and the roles that people play in it. Understanding these messages requires systematic measurement of who is represented. Computer vision face detection tools can provide such measurements; however, state-of-the-art face detection models were trained with photographs, and 80\% of images in children’s books are illustrated; thus existing methods both misclassify and miss classifying many faces. In this paper, we introduce a new approach to analyze images using {AI} tools, resulting in data that can assess representation of race, gender, and age in both illustrations and photographs in children’s books. We make four primary contributions to the fields of deep learning and social sciences: (1) We curate an original face detection data set ({IllusFace} 1.0) by manually labeling 5,403 illustrated faces with bounding boxes. (2) We train two {AutoML}-based face detection models for illustrations: (i) using {IllusFace} 1.0 ({FDAI}); (ii) using {iCartoon}, a publicly available data set ({FDAI} {iC}), each optimized for illustrated images, detecting 2.5 times more faces in our testing data than the established face detector using Google Vision ({FDGV}). (3) We curate a data set of the race, gender, and age of 980 faces manually labeled by three different raters ({CBFeatures} 1.0). (4) We train an {AutoML} feature classification model ({FCA}) using {CBFeatures} 1.0. We compare {FCA} with the performance of another {AutoML} model that we trained on {UTKFace}, a public data set ({FCA} {UTK}) and of an established model using {FairFace} ({FCF}). Finally, we examine distributions of character identities over the last century across the models. We find that {FCA} is 34\% more accurate than {FCF} in its race predictions. These contributions provide tools to educators, caregivers, and curriculum developers to assess the representation contained in children’s content.},
	eventtitle = {2022 {IEEE}/{CVF} Winter Conference on Applications of Computer Vision ({WACV})},
	pages = {3371--3380},
	booktitle = {2022 {IEEE}/{CVF} Winter Conference on Applications of Computer Vision ({WACV})},
	author = {Szasz, Teodora and Harrison, Emileigh and Liu, Ping-Jung and Lin, Ping-Chang and Birali Runesha, Hakizumwami and Adukia, Anjali},
	urldate = {2025-03-30},
	date = {2022-01},
	note = {Annotation: 
    Deep learning for facial feature analysis: This paper introduces new tools for analyzing representation in visual media, particularly illustrated children’s books. The authors train convolutional neural networks using Google {AutoML} to detect and classify faces by age, gender, and race in 1,130 children’s books spanning 1923 to 2019.
    Comparison to traditional methods (including {HyperFace}-like models): The authors demonstrate that traditional face detection methods (e.g., Google Vision) underperform on non-photographic illustrations. Their models ({FDAI} and {FCA}) significantly outperform prior models, detecting more faces and improving classification accuracy, especially for racial identification.
    Application to my research: This is a crucial methodological precedent. The pipeline from detection to classification is directly adaptable to paintings, where the visual style varies significantly from real-world photography. The model here could be one of my choices to detect patterns in gender representations in paintings. Compared to {HyperFace}, this is more suitable for painting data. However, the model did not consist of facial orientation detection, so I might need to add functions to it as well.},
	keywords = {Deep Learning, Feature extraction, Visualization, Deep learning, Predictive models, Accountability, Analysis and Understanding, Data models, Explainable {AI}, Face detection, Fairness, Multimedia Applications, Object Detection/Recognition/Categorization Biometrics -{\textgreater} Face Processing, Privacy and Ethics in Vision, Training data, Visual Reasoning},
	file = {IEEE Xplore Abstract Record:/Users/pootaatoos/Zotero/storage/JT7SX9Z5/9706964.html:text/html;Szasz et al_2022_Measuring Representation of Race, Gender, and Age in Children’s Books.pdf:/Users/pootaatoos/Zotero/storage/XCE4FP94/Szasz et al_2022_Measuring Representation of Race, Gender, and Age in Children’s Books.pdf:application/pdf},
}

@article{charlesworthHistoricalRepresentationsSocial2022,
	title = {Historical representations of social groups across 200 years of word embeddings from Google Books},
	volume = {119},
	url = {https://www.pnas.org/doi/full/10.1073/pnas.2121798119},
	doi = {10.1073/pnas.2121798119},
	abstract = {Using word embeddings from 850 billion words in English-language Google Books, we provide an extensive analysis of historical change and stability in social group representations (stereotypes) across a long timeframe (from 1800 to 1999), for a large number of social group targets (Black, White, Asian, Irish, Hispanic, Native American, Man, Woman, Old, Young, Fat, Thin, Rich, Poor), and their emergent, bottom-up associations with 14,000 words and a subset of 600 traits. The results provide a nuanced picture of change and persistence in stereotypes across 200 y. Change was observed in the top-associated words and traits: Whether analyzing the top 10 or 50 associates, at least 50\% of top associates changed across successive decades. Despite this changing content of top-associated words, the average valence (positivity/negativity) of these top stereotypes was generally persistent. Ultimately, through advances in the availability of historical word embeddings, this study offers a comprehensive characterization of both change and persistence in social group representations as revealed through books of the English-speaking world from 1800 to 1999.},
	pages = {e2121798119},
	number = {28},
	journaltitle = {Proceedings of the National Academy of Sciences},
	author = {Charlesworth, Tessa E. S. and Caliskan, Aylin and Banaji, Mahzarin R.},
	urldate = {2025-03-30},
	date = {2022-07-12},
	note = {Annotation: Historical bias through computational lens: This empirical paper uses word embeddings trained on 850 billion words from Google Books (1800–1999) to study stereotypes toward various social groups, including gender, race, age, and class. The authors analyze associations between social group terms (e.g., “woman,” “Black,” “poor”) and over 14,000 descriptive words, focusing on valence and semantic traits.
Relevance to my study: For my research, this paper provides evidence that long-term gender representations in cultural texts (like books) can be systematically studied using machine learning. While this paper focuses on text data, the methodological framework supports using {ML} tools to examine visual data (like paintings) and trace persistent structural biases in representations.},
	keywords = {not painting},
	file = {Charlesworth et al_2022_Historical representations of social groups across 200 years of word embeddings.pdf:/Users/pootaatoos/Zotero/storage/ULDQPNNK/Charlesworth et al_2022_Historical representations of social groups across 200 years of word embeddings.pdf:application/pdf},
}

@article{butkowskiCriticalExaminationVisualized2017,
	title = {A critical examination of visualized femininity: selective inheritance and intensification of gender posing from historical painting to contemporary advertising},
	volume = {17},
	issn = {1468-0777},
	url = {https://doi.org/10.1080/14680777.2017.1300830},
	doi = {10.1080/14680777.2017.1300830},
	shorttitle = {A critical examination of visualized femininity},
	abstract = {This study investigates the visual correspondences of gender posing behaviors, specifically exhibited by female figures, between historical European painting and contemporary American photographic advertising. While utilizing Erving Goffman’s Gender Advertisements as an analytical framework, the study focuses on the visual legacy of gender ritualization that is salient in contemporary mass media, proposing five semiotic codes to concretely analyze it. Although Goffman’s gender posing is traditionally associated with female vulnerability and indolence, the study proposes that such qualities are exploited as indicators of luxury within Western visual culture and advertising specifically. It seeks to deconstruct and re-frame the interconnected connotations of luxury and gender display, both expanding Goffman’s framework and providing a unique interpretive resource for critical readers of media texts.},
	pages = {1037--1055},
	number = {6},
	journaltitle = {Feminist Media Studies},
	author = {Butkowski, Chelsea P. and Tajima, Atsushi},
	urldate = {2025-03-30},
	date = {2017-11-02},
	note = {Annotation: Linking classical art and modern media through gender analysis based on Goffman's theory: This paper directly investigates how contemporary advertising inherits and intensifies visual gender codes from historical European painting, using Goffman’s "Gender Advertisements" as an analytical base.
    
    Selective visual continuity of gender inequality: The study argues that female figures continue to be represented in “ritualized” postures — head cant, body curvature, gaze aversion—suggesting that cultural ideas of femininity as elegant weakness have been retained and even intensified in mass visual media.
    Relevance to my research: The authors’ interpretive linkage between historical artwork and modern visuals supports my idea of using gendered pose, size, and role as proxies for gender representations in paintings. Their approach also suggests that seemingly aesthetic choices are embedded in long-standing cultural codes of gender hierarchy, supporting my idea of social structure influencing representations in paintings.},
	keywords = {Painting, advertising, Erving Goffman, gender posing, luxury},
	file = {Butkowski_and Tajima_2017_A critical examination of visualized femininity.pdf:/Users/pootaatoos/Zotero/storage/3A7LY7HD/Butkowski_and Tajima_2017_A critical examination of visualized femininity.pdf:application/pdf},
}

@article{kuipersGenderModelsChanging2017,
	title = {Gender models: changing representations and intersecting roles in Dutch and Italian fashion magazines, 1982–2011},
	volume = {26},
	issn = {0958-9236},
	url = {https://doi.org/10.1080/09589236.2016.1155435},
	doi = {10.1080/09589236.2016.1155435},
	shorttitle = {Gender models},
	abstract = {This article presents a comparative content analysis of gender representation in fashion magazines in Italy and the Netherlands. Updating Goffman’s classic study of Gender Advertisements, we study the intersections of gender, professional role, country and time in media representation. Thus, we combine and confront quantitative content analysis with insights from critical gender studies on polysemy and intersectionality. Analyzing a sample of 5840 images from mainstream, commercial and high fashion magazines published in 1982, 1996 and 2011, we find that gender representation strongly intersects with time, place, and notably: professional role. Models, who represent the specific ‘aesthetic capital’ of the fashion field, are portrayed in highly specific ways. Over time, gender differences in representation become stronger in Italy, while in the Netherlands male and female representation converges towards conventionally ‘feminine’ styles. In both countries, we find increasing prominence of (North-American) Goffmanian gender conventions and a new ‘withdrawn’ gendered style emerging in the early 2000s. This new style employs new signs and conventions to denote gender and professional status: it separates men from women, and models from non-models. Our analysis shows, first, that gender representation does not directly reflect gender inequality. Second, it demonstrates the impact of globalization on gender representation. Third, it highlights the polysemy and cultural specificity of visual signs: gender difference can be ‘ritualized’ and ‘stylized’ in various ways. These diverse gendered conventions intersect with other characteristics, and may convey diverse, and changing messages about the relation between gender and sexuality, power, aesthetics and (visual) pleasure.},
	pages = {632--648},
	number = {6},
	journaltitle = {Journal of Gender Studies},
	author = {Kuipers, Giselinde and van der Laan, Elise and Arfini, Elisa A. G.},
	urldate = {2025-03-30},
	date = {2017-11-02},
	note = {Annotation: Cross-national visual content analysis: This paper analyzes over 5,800 images from fashion magazines in Italy and the Netherlands across three decades (1982, 1996, 2011), offering a comparative lens on gender representation. The authors integrate Goffman’s visual semiotics with intersectionality and globalization theory to explore how gender roles in fashion media evolve across time and culture.
Evidence proving different social structures would lead to different gender representations: Though the authors emphasized globalization, the implicit tests on gender representation in different nations, showing that gender representation is embedded in visual culture of different social structures.
Relevance to my research: This study provides an exemplary bridge between sociological visual theory and longitudinal empirical media analysis. Its coding of postures and aesthetic cues directly supports my use of Goffmanian categories to study paintings, while its comparative dimension highlights the importance of cultural and historical context in interpreting gender visuals.},
	keywords = {intersectionality, Italy, fashion magazines, Gender representation, Goffman, Netherlands, not painting},
	file = {Kuipers et al_2017_Gender models.pdf:/Users/pootaatoos/Zotero/storage/KVFDCTM8/Kuipers et al_2017_Gender models.pdf:application/pdf},
}

@article{sariAutomaticDetectionVisualization2019,
	title = {Automatic detection and visualization of garment color in Western portrait paintings},
	volume = {34},
	issn = {2055-7671},
	url = {https://doi.org/10.1093/llc/fqz055},
	doi = {10.1093/llc/fqz055},
	abstract = {Paintings give us important clues about how males and females were perceived over centuries in the Western culture. In this article, we describe a system that allows scholars to automatically visualize how the clothing colors of male and female subjects changed over time. Our system analyzes a large database of paintings, locates portraits, automatically classifies each portrait’s subject as either male or female, segments the clothing areas and finds their dominant color. An interactive, web-based visualization is proposed to allow further exploration of the results. To test the accuracy of our system, we manually annotate a portion of the Rijksmuseum collection, and use state-of-the-art image processing and computer vision algorithms to process the paintings. We use a deep neural network-based style transfer approach to improve gender recognition (or more correctly, sex recognition) of the sitters of portraits. The annotations and the code of the approach are made available.},
	pages = {i156--i171},
	issue = {Supplement\_1},
	journaltitle = {Digital Scholarship in the Humanities},
	shortjournal = {Digital Scholarship in the Humanities},
	author = {Sarı, Cihan and Salah, Albert Ali and Akdag Salah, Alkım Almıla},
	urldate = {2025-03-30},
	date = {2019-12-01},
	keywords = {painting},
	file = {Sarı et al_2019_Automatic detection and visualization of garment color in Western portrait.pdf:/Users/pootaatoos/Zotero/storage/7YDGN4AQ/Sarı et al_2019_Automatic detection and visualization of garment color in Western portrait.pdf:application/pdf;Snapshot:/Users/pootaatoos/Zotero/storage/HYKJUKRE/5556358.html:text/html},
}

@book{goffmanGenderAdvertisements1979,
	location = {New York},
	title = {Gender Advertisements},
	isbn = {978-0-06-132076-7},
	abstract = {This book looks at the behavioral representation of our cultural assumptions about the nature of the sexes. It explores the properties of gender displays, with the nature of those interpersonal rituals through which we affirm in daily life our apparent beliefs regarding the character of males and females and the relationship that is approved within and across sex status. It also discusses the nature of photography and the relations of photographs to what they purport to picture. The author also details the ways in which gender, especially female gender, is presented in popular advertisements.},
	pagetotal = {108},
	publisher = {Harper \& Row},
	author = {Goffman, Erving},
	date = {1979},
	langid = {english},
	note = {Annotation: Fundamental theory for gender representation: Speaks about how different genders are represented differently in advertisements. Goffman emphasized the gender differences in visual materials, and had a detailed typology of these differences in this book. 
Detailed Typology: The typology could be used as the measurements for my research, including Relative Size, Feminine Touch, Function Ranking, Ritualization of Subordination, Licensed Withdrawal, Infantilization, Body Cant, Head Cant, Eye Aversion, Self-Touching, and Family Roles. 
Application to my research: These types of female appearance in advertisement concluded by Goffman are not exclusive to each other, from my perspective. I would conclude them into three categories: Size (the percentage of painting pixels occupied by females compared to males), Role (the function of females in the paintings compared to males), and Posture (physical orientation or activity of characters).},
	keywords = {Social Science / General, Business \& Economics / Advertising \& Promotion, Social Science / Gender Studies},
	file = {Goffman_1979_Gender Advertisements.pdf:/Users/pootaatoos/Zotero/storage/K9LGFW8N/Goffman_1979_Gender Advertisements.pdf:application/pdf},
}

@article{smithFacadeNeutralityUnearthing2023,
	title = {The Façade of Neutrality: Unearthing Hidden Histories in the Montias Database with Digital Methodologies},
	url = {https://jhna.org/articles/facade-of-neutrality-unearthing-hidden-histories-in-the-montias-database-with-digital-methodologies/},
	doi = {10.5092/jhna.2023.15.1.8},
	shorttitle = {The Façade of Neutrality},
	abstract = {Technological innovations and the 2020 global pandemic have increased scholarly interactions with and reliance on digital resources, leading cultural heritage institutions to prioritize initiatives that digitize and organize their collections online, aiding in their accessibility to a wider global audience. Focusing on the Montias Database of 17th Century Art Inventories, this article turns a critical eye toward digitized art-historical data that is collected, preserved, and organized by cultural heritage institutions. Using the digital art history project “Beyond ‘Exceptional’: Reassessing Women’s Participation in the Cultural Sphere of Seventeenth-Century Amsterdam with Digital Social Network Analysis,” I demonstrate the potential of computational methods for mediating the biases inherent to these collections.},
	journaltitle = {Journal of Historians of Netherlandish Art},
	shortjournal = {{JHNA}},
	author = {Smith, Lauryn},
	urldate = {2025-03-30},
	date = {2023-03},
	langid = {english},
	note = {Annotation: mpirical example of artist network showing hidden power structure: This research applies social network analysis to the Montias Database of 17th-century Dutch art inventories to reveal the overlooked contributions of women in Amsterdam’s cultural sphere. The study constructs a network graph of over 2,200 nodes, identifying women as key cultural agents who were previously obscured due to male-centric cataloging. Also, the study visualizes how individuals like Catharina van der Voort acted as central hubs within art and collecting networks, challenging the assumed invisibility of women in historical art economies.
Relevance to my research: This study provides a concrete model for applying network centrality to historical artistic contexts, validating the use of {SNA} methods in uncovering gendered dynamics. It reinforces my approach of measuring artists’ structural power via social centrality and linking this to their visual representation of gender in art.
Research gap: This paper did not pay attention to the cultural productions, while only focused on the network phenomenon. My research would like to extend the approach to explore how centrality and centralization would influence the real cultural productions.},
